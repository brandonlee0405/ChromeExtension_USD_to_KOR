{
  "manifest_version": 2,

  "name": "USD to KOR",
  "description": "This extension shows the KOR currency over the USD currency on a page",
  "version": "1.0",

  "background" : {
    "scripts" : ["content.js"]
  },

  "options_ui": {
      "page": "options.html",

      "chrome_style": true
    },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
